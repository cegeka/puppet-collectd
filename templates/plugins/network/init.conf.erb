LoadPlugin network
<Plugin network>
<%- scope.lookupvar('network_items').each do |network_ip,value| -%>
   <Server "<%= network_ip %>" "<%= value['exporter_port'] %>">
     SecurityLevel <%= value['exporter_securitylevel'] %>
     Username "<%= value['exporter_username'] %>"
     Password "<%= value['exporter_password'] %>"
   </Server>
<%- end -%>
</Plugin>
