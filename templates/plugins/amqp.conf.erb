LoadPlugin amqp
<Plugin "amqp">
  <Publish "<%= exchange %>">
    Host "<%= host %>"
    Port "<%= port %>"
    VHost "/"
    User "<%= user %>"
    Password "<%= password %>"
    Exchange "<%= exchange %>"
    RoutingKey ""
    Persistent false
    Format "<%= messageFormat %>"
    GraphitePrefix "<%= graphitePrefix %>"
    StoreRates false
 </Publish>
</Plugin>
